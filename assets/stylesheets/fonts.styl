@import 'icon_font.css'

// Icons for weather fonts
@font-face
    font-family MeteoconsRegular
    src url('/fonts/meteocons/meteocons-webfont.eot'), \
        url('/fonts/meteocons/meteocons-webfont.eot?#iefix') format('embedded-opentype'), \
        url('/fonts/meteocons/meteocons-webfont.woff') format('woff'), \
        url('/fonts/meteocons/meteocons-webfont.ttf') format('truetype'), \
        url('/fonts/meteocons/meteocons-webfont.svg#MeteoconsRegular') format('svg')
    font-weight normal
    font-style normal

.weather-icon
  font-family MeteoconsRegular


@font-face
  font-family: aleolight
  src url('/fonts/aleo/aleo-light-webfont.eot'), \
      url('/fonts/aleo/aleo-light-webfont.eot?#iefix') format('embedded-opentype'), \
      url('/fonts/aleo/aleo-light-webfont.woff') format('woff'), \
      url('/fonts/aleo/aleo-light-webfont.ttf') format('truetype'), \
      url('/fonts/aleo/aleo-light-webfont.svg#aleolight') format('svg')

@font-face
  font-family: aleolightitalic
  src url('/fonts/aleo/aleo-lightitalic-webfont.eot'), \
      url('/fonts/aleo/aleo-lightitalic-webfont.eot?#iefix') format('embedded-opentype'), \
      url('/fonts/aleo/aleo-lightitalic-webfont.woff') format('woff'), \
      url('/fonts/aleo/aleo-lightitalic-webfont.ttf') format('truetype'), \
      url('/fonts/aleo/aleo-lightitalic-webfont.svg#aleolightitalic') format('svg')


@font-face
  font-family: aleobold
  src url('/fonts/aleo/aleo-bold-webfont.eot'), \
      url('/fonts/aleo/aleo-bold-webfont.eot?#iefix') format('embedded-opentype'), \
      url('/fonts/aleo/aleo-bold-webfont.woff') format('woff'), \
      url('/fonts/aleo/aleo-bold-webfont.ttf') format('truetype'), \
      url('/fonts/aleo/aleo-bold-webfont.svg#amaticbold') format('svg')

@font-face
  font-family: amatic-bold
  src url('/fonts/amatic/amatic-bold-webfont.eot'), \
      url('/fonts/amatic/amatic-bold-webfont.eot?#iefix') format('embedded-opentype'), \
      url('/fonts/amatic/amatic-bold-webfont.woff') format('woff'), \
      url('/fonts/amatic/amatic-bold-webfont.ttf') format('truetype'), \
      url('/fonts/amatic/amatic-bold-webfont.svg#amaticbold') format('svg')

.aleo-light-italic
  font-family: aleolightitalic

.aleo-bold
  font-family aleobold

.aleo-light
  font-family: aleolight

.amatic-bold
  font-family: amatic-bold


